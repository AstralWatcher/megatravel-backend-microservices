<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="ExampleWebServiceInterface"
	targetNamespace="http://megatravel.com/example"
	xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:tns="http://megatravel.com/example"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
	<wsdl:message name="displayDataResponse">
		<wsdl:part name="return" type="xsd:int">
		</wsdl:part>
	</wsdl:message>
	<wsdl:message name="displayData">
		<wsdl:part name="name" type="xsd:string">
		</wsdl:part>
		<wsdl:part name="years" type="xsd:int">
		</wsdl:part>
	</wsdl:message>
	<wsdl:portType name="ExampleWebServiceInterface">
		<wsdl:operation name="displayData">
			<wsdl:input name="displayData" message="tns:displayData">
			</wsdl:input>
			<wsdl:output name="displayDataResponse"
				message="tns:displayDataResponse">
			</wsdl:output>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding
		name="ExampleWebServiceInterfaceServiceSoapBinding"
		type="tns:ExampleWebServiceInterface">
		<soap:binding style="rpc"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="displayData">
			<soap:operation soapAction="" style="rpc" />
			<wsdl:input name="displayData">
				<soap:body use="literal"
					namespace="http://megatravel.com/example" />
			</wsdl:input>
			<wsdl:output name="displayDataResponse">
				<soap:body use="literal"
					namespace="http://megatravel.com/example" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="ExampleWebServiceInterface">
		<wsdl:port name="ExampleWebServicePort"
			binding="tns:ExampleWebServiceInterfaceServiceSoapBinding">
			<soap:address
				location="http://localhost:9090/ExampleWebServicePort" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
